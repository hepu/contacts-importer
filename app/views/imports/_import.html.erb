<tr>
  <td><%= link_to import.id, import_path(import) %></td>
  <td>
    <%= turbo_stream_from import %>
    <%= turbo_frame_tag "#{dom_id(import)}_status" do %>
      <%= render 'status', import: import %>
    <% end %>
  </td>
  <td><%= link_to import.created_at.strftime('%Y/%m/%d'), import_path(import) %></td>
</tr>